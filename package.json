{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:test": "NODE_ENV=test ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:prod": "NODE_ENV=production ts-node-dev --respawn --transpile-only src/server.ts",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "start": "NODE_ENV=production node dist/server.js",
    "start:dev": "NODE_ENV=development node dist/server.js",
    "start:test": "NODE_ENV=test node dist/server.js",
    "test": "NODE_ENV=test jest --runInBand",
    "test:watch": "NODE_ENV=test jest --runInBand --watch",
    "clear-test": "jest --clearCache",
    "prepare": "husky",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "NODE_ENV=development prisma migrate deploy",
    "db:migrate:test": "NODE_ENV=test prisma migrate deploy",
    "db:migrate:prod": "NODE_ENV=production prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:seed": "NODE_ENV=development ts-node-dev prisma/seed.ts",
    "db:seed:test": "NODE_ENV=test ts-node-dev prisma/seed.ts",
    "db:reset": "NODE_ENV=development prisma migrate reset --force",
    "db:reset:test": "NODE_ENV=test prisma migrate reset --force --skip-seed"
  },
  "prisma": {
    "seed": "ts-node-dev prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.10.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.3",
    "@types/supertest": "^6.0.3",
    "husky": "^9.1.7",
    "jest": "^30.0.2",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
